http://*.mooo.com {
        @apps path_regexp app ^/(\w+)

        handle @apps {
                uri strip_prefix /{re.app.1}
                reverse_proxy {labels.2}-{re.app.1}-1:9876
        }

        handle {
                respond "Welcome to {host}" 200
        }

        handle_errors 502 {
                respond "Not found" 404
        }
}
